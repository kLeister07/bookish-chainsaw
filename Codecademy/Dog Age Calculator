//this is my age
let myAge = 11
;
//this is for calculating your age into early dog years
let earlyYears = 2;
earlyYears *= 10.5;
//this is for calculating later dog years
let laterYears = myAge - 2;
//calculation code for later years continued
laterYears *= 4;
let myAgeInDogYears = earlyYears + laterYears;
//This is my name
let myName = "Kevin".toLowerCase();
console.log("My name is " + myName + ". I am " + myAge + " years old in human years which is " + myAgeInDogYears + " years old in dog years." );